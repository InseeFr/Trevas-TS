{
	"name": "trevas-ts-documentation",
	"description": "Trevas TS documentation",
	"version": "0.0.1",
	"license": "MIT",
	"engines": {
		"node": ">=18.0",
		"npm": ">=8.0.0"
	},
	"scripts": {
		"docusaurus": "docusaurus",
		"start": "docusaurus start",
		"start-fr": "docusaurus start --locale fr",
		"start-zh-CN": "docusaurus start --locale zh-CN",
		"start-no": "docusaurus start --locale no",
		"build": "docusaurus build",
		"swizzle": "docusaurus swizzle",
		"deploy": "docusaurus deploy",
		"postinstall": "yarn write-translations",
		"format:js": "prettier --loglevel warn --write \"src/**/*.{js,ts,jsx,tsx}\" \"docs/**/*.{md,mdx}\"",
		"format:docs": "prettier --loglevel warn --write \"docs/**/*.{md,mdx}\"",
		"format": "yarn format:js && yarn format:docs",
		"lint:fix": "eslint \"src/**/*.{js,jsx,ts,tsx}\" --parser-options=project:tsconfig.json --fix",
		"lint": "eslint \"src/**/*.{js,jsx,ts,tsx}\" --parser-options=project:tsconfig.json",
		"clear": "docusaurus clear",
		"serve": "docusaurus serve",
		"build-serve": "yarn build && yarn serve",
		"write-translations": "docusaurus write-translations & yarn format:docs"
	},
	"dependencies": {
		"@docusaurus/core": "^3.5.2",
		"@docusaurus/plugin-content-pages": "^3.5.2",
		"@docusaurus/plugin-sitemap": "^3.5.2",
		"@docusaurus/preset-classic": "^3.5.2",
		"@docusaurus/theme-search-algolia": "^3.5.2",
		"@mdx-js/react": "^3.0.1",
		"clsx": "^2.1.1",
		"docusaurus-lunr-search": "^3.5.0",
		"docusaurus-plugin-sass": "^0.2.2",
		"prism-react-renderer": "^2.4.0",
		"react": "^18.3.1",
		"react-dom": "^18.3.1",
		"react-scroll": "^1.8.8",
		"react-slick": "^0.30.2",
		"sass": "^1.77.8",
		"typed.js": "^2.0.12"
	},
	"devDependencies": {
		"@babel/cli": "^7.22.20",
		"@babel/core": "^7.22.20",
		"@babel/plugin-transform-runtime": "^7.22.20",
		"@babel/preset-react": "^7.22.15",
		"@babel/types": "^7.25.6",
		"@docusaurus/module-type-aliases": "^3.5.2",
		"@tsconfig/docusaurus": "^2.0.3",
		"@types/react-slick": "^0.23.13",
		"@typescript-eslint/eslint-plugin": "^8.4.0",
		"@typescript-eslint/parser": "^8.4.0",
		"docusaurus-node-polyfills": "^1.0.0",
		"eslint": "^9.9.1",
		"eslint-config-prettier": "^9.0.0",
		"eslint-plugin-import": "^2.30.0",
		"eslint-plugin-prettier": "^5.2.1",
		"eslint-plugin-simple-import-sort": "^12.1.1",
		"prettier": "^3.3.3",
		"prettier-linter-helpers": "^1.0.0",
		"typescript": "^5.5.4"
	},
	"browserslist": {
		"production": [
			">0.5%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	}
}
