{
	"extends": "@tsconfig/docusaurus/tsconfig.json",
	"compilerOptions": {
		"baseUrl": ".",
		"paths": {
			"@site/*": ["./*"],
			"@theme/*": ["./src/theme/*"]
		},
		"resolveJsonModule": true
	},
	"include": ["./src/**/*.ts", "./src/**/*.tsx"],
	"exclude": ["node_modules", "build", "dist", "src/**/*.js", "src/**/*.jsx"]
}
